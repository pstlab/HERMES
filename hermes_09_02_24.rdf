    <!-- http://www.istc.cnr.it/pstlab/ontologies/2023/1/hermes#ResidenzaDiElena -->

    <owl:NamedIndividual rdf:about="http://www.istc.cnr.it/pstlab/ontologies/2023/1/hermes#ResidenzaDiElena">
        <rdf:type rdf:resource="http://www.istc.cnr.it/pstlab/ontologies/2023/1/hermes#Residenza"/>
