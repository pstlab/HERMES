    <!-- http://www.istc.cnr.it/pstlab/ontologies/2023/1/hermes#DomusRitrattiFontana -->

    <owl:NamedIndividual rdf:about="http://www.istc.cnr.it/pstlab/ontologies/2023/1/hermes#DomusRitrattiFontana">
        <rdf:type rdf:resource="http://www.istc.cnr.it/pstlab/ontologies/2023/1/hermes#Residenza"/>
        <core:isPartOf rdf:resource="http://www.istc.cnr.it/pstlab/ontologies/2023/1/hermes#SantaCroceInGerusalemme"/>
