    <owl:NamedIndividual rdf:about="http://www.istc.cnr.it/pstlab/ontologies/2023/1/hermes#ColleEsquilino">
        <rdf:type rdf:resource="https://w3id.org/arco/ontology/arco/ArchitecturalOrLandscapeHeritage"/>
